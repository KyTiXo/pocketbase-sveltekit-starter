{
  "name": "sk",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "dev": "vite dev",
    "dev:backend": "cd ../pb && modd",
    "backend": "AUDITLOG=users,posts ../pb/entrypoint.sh ./pocketbase serve --publicDir=../sk/build",
    "build": "vite build",
    "build:backend": "cd ../pb && go build",
    "typegen": "pocketbase-typegen --db ../pb/pb_data/data.db --out ./src/lib/pocketbase/generated-types.ts",
    "test": "playwright test",
    "preview": "vite preview",
    "check": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json",
    "check:watch": "svelte-kit sync && svelte-check --tsconfig ./tsconfig.json --watch",
    "lint": "prettier --check .",
    "format": "prettier --write ."
  },
  "devDependencies": {
    "@sveltejs/adapter-auto": "^3.3.1",
    "@sveltejs/adapter-static": "^3.0.8",
    "@sveltejs/kit": "^2.15.2",
    "@sveltejs/vite-plugin-svelte": "^5.0.3",
    "@tailwindcss/postcss": "4.0.0-beta.9",
    "@tailwindcss/typography": "^0.5.16",
    "@types/node": "^22.10.6",
    "autoprefixer": "^10.4.20",
    "pocketbase-typegen": "^1.3.0",
    "postcss": "^8.5.0",
    "prettier": "^3.4.2",
    "prettier-plugin-svelte": "^3.3.3",
    "prettier-plugin-tailwindcss": "^0.6.9",
    "svelte": "^5.17.4",
    "svelte-check": "^3.8.6",
    "svelte-preprocess": "^5.1.4",
    "tailwindcss": "4.0.0-beta.9",
    "tslib": "^2.8.1",
    "typescript": "^5.7.3",
    "vite": "^6.0.7"
  },
  "type": "module",
  "dependencies": {
    "@tailwindcss/vite": "4.0.0-beta.9",
    "daisyui": "5.0.0-beta.1",
    "lucide-svelte": "^0.471.0",
    "pocketbase": "^0.21.5",
    "zod": "^3.24.1"
  }
}
